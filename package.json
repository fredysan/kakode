{
    "name": "kakode",
    "displayName": "kakode",
    "description": "Kakoune mode for Visual Studio Code",
    "version": "0.0.1",
    "author": "glennsl",
    "license": "MIT",
    "publisher": "",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./lib/js/src/extension",
    "contributes": {
        "keybindings": [{
            "command": "kak.escape",
            "key": "escape",
            "when": "editorTextFocus"
        }]
    },
    "scripts": {
        "vscode:prepublish": "bsb -make-world",
        "compile": "bsb -make-world -w",
        "clean": "bsb -clean-world",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "bs-promise": "buckletypes/bs-promise",
        "rebase": "glennsl/rebase"
    },
    "devDependencies": {
        "vscode": "^1.0.0",
        "bs-platform": "bloomberg/bucklescript"
    }
}